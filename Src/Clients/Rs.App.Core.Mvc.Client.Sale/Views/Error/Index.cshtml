@model Rs.App.Core.Mvc.Client.Sale.Models.StatusCodeModel
@{
    ViewData["Title"] = "Status Code @Model.ErrorStatusCode";
}

<h1 class="text-danger">Status Code: @Model.ErrorStatusCode : @Model.Message</h1>
<h2 class="text-danger">An error occurred while processing your request.</h2>

@if (Model.ShowRequestId)
{
    <h3>Request ID</h3>
    <p>
        <code>@Model.RequestId</code>
    </p>
}


